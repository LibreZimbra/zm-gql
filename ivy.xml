<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info organisation="zimbra" module="zm-gql" status="integration" />
    <configurations>
      <conf name="compile" />
      <conf name="runtime" extends="compile" />
      <conf name="test" extends="runtime" />
      <conf name="doclet" extends="runtime" />
    </configurations>
    <dependencies>
      <!-- Core Requirements -->
      <dependency org="com.graphql-java" name="graphql-java" rev="7.0" conf="compile->*" />
      <!-- Test Requirements -->
      <dependency org="junit" name="junit" rev="4.12" conf="test->*" />
      <dependency org="org.easymock" name="easymock" rev="3.4" conf="test->*" />
      <!-- Doclet Requirements -->
      <dependency org="com.tenxerconsulting" name="swagger-doclet" rev="1.1.3" conf="doclet->*" />
    </dependencies>
</ivy-module>